<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sfc="http://schemas.spatialeye.com/clienttoolkit"
    x:Class="Lite.LitePrintA4Template3HeaderPageView"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400" Width="500" Height="500">

  <Grid x:Name="LayoutRoot" Background="White">
    <TextBlock TextWrapping="Wrap" Text="{Binding LiteName, Source={StaticResource Locator}}" Margin="20,0" FontSize="36" d:LayoutOverrides="Width, Height" HorizontalAlignment="Center" VerticalAlignment="Center"/>
  </Grid>
</UserControl>
